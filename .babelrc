{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
       { "legacy": true }
    ],
    [
      "@babel/plugin-syntax-throw-expressions",
      { "legacy": true }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      { "legacy": true }
    ]
  ]
}
